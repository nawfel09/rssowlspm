            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
         <attribute name="id" type="string">
            <annotation>
               <documentation>
                  
               </documentation>
            </annotation>
         </attribute>
         <attribute name="name" type="string">
            <annotation>
               <documentation>
                  
               </documentation>
               <appInfo>
                  <meta.attribute translatable="true"/>
               </appInfo>
            </annotation>
         </attribute>
      </complexType>
   </element>

   <element name="namespaceHandler">
      <annotation>
         <appInfo>
            <meta.element labelAttribute="namespaceURI"/>
         </appInfo>
      </annotation>
      <complexType>
         <attribute name="namespaceURI" type="string" use="required">
