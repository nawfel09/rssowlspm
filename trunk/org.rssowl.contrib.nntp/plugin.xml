<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.rssowl.core.ProtocolHandler">
      <protocolHandler
            class="org.rssowl.contrib.internal.nntp.NewsGroupHandler"
            protocol="nntp">
      </protocolHandler>
      <protocolHandler
            class="org.rssowl.contrib.internal.nntp.NewsGroupHandler"
            protocol="news">
      </protocolHandler>
   </extension>
   <extension
         point="org.rssowl.core.CredentialsProvider">
      <credentialsProvider
            class="org.rssowl.contrib.internal.nntp.NewsGroupCredentialsProvider"
            protocol="nntp">
      </credentialsProvider>
      <credentialsProvider
            class="org.rssowl.contrib.internal.nntp.NewsGroupCredentialsProvider"
            protocol="news">
      </credentialsProvider>
   </extension>

</plugin>
